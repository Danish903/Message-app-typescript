{
   "compilerOptions": {
      "allowJs": false,
      "allowSyntheticDefaultImports": true,
      "noFallthroughCasesInSwitch": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "noUnusedParameters": true,
      "jsx": "react-native",
      "lib": ["es6"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "esnext"
   },
   "exclude": ["node_modules"]
}
